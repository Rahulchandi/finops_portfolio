SELECT 
    line_item_resource_id AS volume_id,
    SUM(line_item_unblended_cost) AS snapshot_cost
FROM "aws_cur_database"."cur_table"
WHERE line_item_product_code = 'AmazonEC2'
  AND line_item_usage_type LIKE '%Snapshot%'
GROUP BY volume_id
HAVING SUM(line_item_unblended_cost) > 0
ORDER BY snapshot_cost DESC;
